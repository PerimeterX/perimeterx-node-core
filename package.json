        "uuid": "2.0.2"
    },
    "devDependencies": {
        "@types/node": "^18.11.17",
        "eslint": "^6.8.0",
        "eslint-config-perimeterx": "^0.3.0",
        "mocha": "^7.1.1",
        "babel-preset-env": "^1.7.0",
        "proxyquire": "^2.1.3",
        "rewire": "^2.5.2",
        "should": "^8.3.0",

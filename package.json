    "description": "PerimeterX NodeJS shared core for various applications to monitor and block traffic according to PerimeterX risk score",
    "main": "index.js",
    "scripts": {
        "test": "mocha",
        "cover": "istanbul cover _mocha",
        "lint": "./node_modules/eslint/bin/eslint.js lib/",
